tom align=\"center\">\n          도서 수정\n        </Typography>\n\n        {error && (\n          <Alert severity=\"error\" sx={{ mb: 2 }} onClose={() => setError('')}>\n            {error}\n          </Alert>\n        )}\n\n        {success && (\n          <Alert severity=\"success\" sx={{ mb: 2 }} onClose={() => setSuccess('')}>\n            {success}\n          </Alert>\n        )}\n\n        <Box component=\"form\" onSubmit={handleSubmit} sx={{ mt: 3 }}>\n          <TextField\n            fullWidth\n            label=\"제목\"\n            name=\"title\"\n            value={formData.title}\n            onChange={handleInputChange}\n            required\n            margin=\"normal\"\n            placeholder=\"도서 제목을 입력하세요\"\n          />\n\n          <TextField\n            fullWidth\n            label=\"내용\"\n            name=\"content\"\n            value={formData.content}\n            onChange={handleInputChange}\n            required\n            margin=\"normal\"\n            multiline\n            rows={6}\n            placeholder=\"도서 내용을 입력하세요\"\n          />\n\n          <FormControl component=\"fieldset\" sx={{ mt: 3, mb: 2 }}>\n            <FormLabel component=\"legend\">표지 이미지 선택 방식</FormLabel>\n            <RadioGroup\n              row\n              name=\"coverImageType\"\n              value={formData.coverImageType}\n              onChange={handleInputChange}\n            >\n              <FormControlLabel\n                value=\"existing\"\n                control={<Radio />}\n                label=\"기존 이미지 유지\"\n              />\n              <FormControlLabel\n                value=\"upload\"\n                control={<Radio />}\n                label=\"새로 업로드\"\n              />\n              <FormControlLabel\n                value=\"ai\"\n                control={<Radio />}\n                label=\"AI 생성\"\n              />\n            </RadioGroup>\n          </FormControl>\n\n          {formData.coverImageType === 'upload' && (\n            <Box sx={{ mt: 2 }}>\n              <Button\n                variant=\"outlined\"\n                component=\"label\"\n                startIcon={<CloudUploadIcon />}\n                fullWidth\n              >\n                이미지 업로드\n                <input\n                  type=\"file\"\n                  hidden\n                  accept=\"image/*\"\n                  onChange={handleImageUpload}\n                />\n              </Button>\n              {uploadedImage && (\n                <Typography variant=\"body2\" sx={{ mt: 1, color: 'text.secondary' }}>\n                  선택된 파일: {uploadedImage.name}\n                </Typography>\n              )}\n            </Box>\n          )}\n\n          {formData.coverImageType === 'ai' && (\n            <Box sx={{ mt: 2 }}>\n              <TextField\n                fullWidth\n                label=\"AI 표지 생성 프롬프트\"\n                value={aiPrompt}\n                onChange={(e) => setAiPrompt(e.target.value)}\n                placeholder=\"예: 신비로운 숲 속의 오래된 도서관\"\n                margin=\"normal\"\n              />\n              <Button\n                variant=\"outlined\"\n                startIcon={aiGenerating ? <CircularProgress size={20} /> : <AutoAwesomeIcon />}\n                onClick={handleGenerateAI}\n                disabled={aiGenerating || !aiPrompt.trim()}\n                fullWidth\n                sx={{ mt: 1 }}\n              >\n                {aiGenerating ? 'AI 표지 생성 중...' : 'AI 표지 생성'}\n              </Button>\n            </Box>\n          )}\n\n          {previewImage && (\n            <Card sx={{ mt: 3, maxWidth: 400, mx: 'auto' }}>\n              <CardMedia\n                component=\"img\"\n                image={previewImage}\n                alt=\"표지 미리보기\"\n                sx={{ height: 300, objectFit: 'cover' }}\n              />\n              <Typography variant=\"caption\" sx={{ p: 1, display: 'block', textAlign: 'center' }}>\n                {formData.coverImageType 